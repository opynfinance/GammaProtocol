{
  "extends": "solhint:recommended",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "var-name-mixedcase": "off",
    "mark-callable-contracts": "off",
    "compiler-version":["error", "0.6.10"],
    "not-rely-on-time": "off",
    "no-complex-fallback": "off",
    "no-inline-assembly": "off",
    "reason-string": "off"
  }
}
